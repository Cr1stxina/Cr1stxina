programa {
  funcao inicio() {
    
    //Declaração de variáveis
    inteiro numeros[10], maior, menor

    //Laço de repetição (PARA) para preenchimento das 10 posições do vetor com números
    para ( inteiro i = 0 ; i < 10 ; i++) {
    escreva("Digite um número: ")
    leia(numeros[i])
    }

    //Inicialização do maior e menor com o primeiro valor do vetor
    maior = numeros[0]
    menor = numeros[0]

    //Laço de repetição (PARA) e teste (SE) para encontrar o maior e o menor número dentro das 10 posições do vetor
    para (inteiro i = 0; i < 10; i++) {
      se (numeros[i] > maior) {
        maior = numeros[i]
      }
      se (numeros[i] < menor) {
        menor = numeros[i]
      }
    }

    //Exibição na tela do maior e do menor número dentro das 10 posições do vetor
    escreva("\nO maior número é: ", maior, "\n" ,"E o menor número é: ", menor, "\n" )
  }
}
